<?xml version="1.0" encoding="UTF-8"?>

<!--
    Document   : ExpressionLevelDesc.hbm.xml
    Created on : July 19, 2005
    Author     : rajputs
    Description: Hibernate mapping file for ExpressionLevelDesc
-->

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.camod.domain">
	<class name="ExpressionLevelDesc" 
    	table="exp_level_desc"
        lazy="true" mutable="false">

	    <!-- id -->
	    <id name="id" 
	    	column="exp_level_desc_id"
	    	type="long"    	
	    	unsaved-value="null">	    	
	        <generator class="native"/>
	    </id>
	    
	    <!-- expressionLevel -->
        <property name="expressionLevel" 
        	column="expression_level" 
        	type="string"/>
	</class>
</hibernate-mapping>