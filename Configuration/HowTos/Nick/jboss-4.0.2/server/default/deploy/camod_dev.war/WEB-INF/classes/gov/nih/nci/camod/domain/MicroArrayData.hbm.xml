    Created on : May 4, 2005
    Author     : rajputs
    Description: Hibernate mapping file for MicroArrayData
-->

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.camod.domain">
	<class name="MicroArrayData"
		table="micro_array_data"
        lazy="true">
	    
	    <!-- id -->
	    <id name="id" 
	    	column="micro_array_data_id"
	    	type="long"    	
	    	unsaved-value="null">	    	
	        <generator class="native"/>
	    </id>	
	    
        <!-- experimentName -->
        <property name="experimentName" 
        	column="experiment_name" 
        	type="string"/>
        	
        <!-- experimentId -->
        <property name="experimentId" 
        	column="experiment_id"
        	type="long"/>
        	
        <!-- otherLocationURL -->
        <property name="otherLocationURL" 
        	column="other_location_url" 
        	type="string"/>
        	

	</class>
</hibernate-mapping>